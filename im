   "cell_type": "code",
   "execution_count": 17,
   "id": "7be99984",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Dataset loaded successfully.\n",
      "Columns: ['Country', 'Year', 'Food Category', 'Total Waste (Tons)', 'Economic Loss (Million $)', 'Avg Waste per Capita (Kg)', 'Population (Million)', 'Household Waste (%)']\n",
      "Renamed columns: ['country', 'year', 'food_category', 'total_waste_tons', 'economic_loss_million_usd', 'avg_waste_per_capita', 'population_millions', 'Household Waste (%)']\n",
      "Data preprocessing completed. Cleaned CSV saved to 'data/cleaned_food_wastage.csv'\n",
      "\n"
     ]
    }
   ],
   "source": [
    "# full_project.py\n",
    "\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.linear_model import LinearRegression\n",
    "from sklearn.metrics import mean_squared_error\n",
    "import os\n",
    "\n",
    "# -----------------------------\n",
    "# 1. Load & preprocess data\n",
    "# -----------------------------\n",
    "# Ensure 'data' folder exists\n",
    "os.makedirs(\"data\", exist_ok=True)\n",
    "\n",
    "# Load dataset\n",
    "df = pd.read_csv(\"C:/Users/dell/Downloads/global_food_wastage_dataset.csv\")\n",
    "print(\"Dataset loaded successfully.\\nColumns:\", df.columns.tolist())\n",
    "\n",
    "# Automatic column mapping\n",
    "column_mapping = {}\n",
    "for col in df.columns:\n",
    "    col_lower = col.strip().lower().replace(\" \", \"_\")\n",
    "    if \"country\" in col_lower:\n",
    "        column_mapping[col] = \"country\"\n",
    "    elif \"food\" in col_lower and \"category\" in col_lower:\n",
    "        column_mapping[col] = \"food_category\"\n",
    "    elif \"year\" in col_lower:\n",
    "        column_mapping[col] = \"year\"\n",
    "    elif \"total\" in col_lower and \"waste\" in col_lower:\n",
    "        column_mapping[col] = \"total_waste_tons\"\n",
    "    elif \"economic\" in col_lower and \"loss\" in col_lower:\n",
    "        column_mapping[col] = \"economic_loss_million_usd\"\n",
    "    elif \"avg\" in col_lower and \"waste\" in col_lower:\n",
    "        column_mapping[col] = \"avg_waste_per_capita\"\n",
    "    elif \"population\" in col_lower:\n",
    "        column_mapping[col] = \"population_millions\"\n",
    "\n",
    "df.rename(columns=column_mapping, inplace=True)\n",
    "print(\"Renamed columns:\", df.columns.tolist())\n",
    "\n",
    "# Check essential columns\n",
    "essential_cols = [\"total_waste_tons\", \"avg_waste_per_capita\", \"population_millions\", \"year\"]\n",
    "for col in essential_cols:\n",
    "    if col not in df.columns:\n",
    "        raise Exception(f\"Missing required column: {col}\")\n",
    "\n",
    "# Convert year to numeric\n",
    "df[\"year\"] = pd.to_numeric(df[\"year\"], errors='coerce')\n",
    "df.dropna(subset=[\"year\"], inplace=True)\n",
    "df[\"year\"] = df[\"year\"].astype(int)\n",
    "\n",
    "# Drop rows with missing numeric data\n",
    "df.dropna(subset=[\"total_waste_tons\", \"avg_waste_per_capita\", \"population_millions\"], inplace=True)\n",
    "\n",
    "# Remove zero population\n",
    "df = df[df[\"population_millions\"] > 0]\n",
    "\n",
    "# Feature engineering\n",
    "df[\"waste_per_million_pop\"] = df[\"total_waste_tons\"] / (df[\"population_millions\"] * 1_000_000) * 1_000_000\n",
    "\n",
    "# Save cleaned dataset\n",
    "df.to_csv(\"data/cleaned_food_wastage.csv\", index=False)\n",
    "print(\"Data preprocessing completed. Cleaned CSV saved to 'data/cleaned_food_wastage.csv'\\n\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "da769f8a",
